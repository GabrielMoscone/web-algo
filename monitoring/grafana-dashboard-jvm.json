          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "(jvm_memory_used_bytes{job=\"middleware\",instance=\"middleware1:8081\",area=\"heap\"} / jvm_memory_max_bytes{job=\"middleware\",instance=\"middleware1:8081\",area=\"heap\"}) * 100",
          "legendFormat": "Heap Used %",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 6
