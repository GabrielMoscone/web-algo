{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Fonte de dados Prometheus com m√©tricas do k6",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "9.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "table",
      "name": "Table",
      "version": ""
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "2.0.0"
    }
  ],
  "id": null,
  "uid": null,
  "title": "Web-Algo - Performance Monitor",
  "tags": ["k6", "performance", "web-algo", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-30m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
  },
  "panels": [
    {
      "id": 1,
      "title": "üìä Requisi√ß√µes por Segundo (RPS)",
      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "sum(rate(k6_http_reqs_total[1m]))",
          "legendFormat": "RPS",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          },
          "unit": "reqps"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center"
      }
    },
    {
      "id": 2,
      "title": "üë• Usu√°rios Simult√¢neos",
      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "max(k6_vus)",
          "legendFormat": "VUs",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 10},
              {"color": "yellow", "value": 50},
              {"color": "red", "value": 100}
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "‚ö° Tempo de Resposta (ms)",
      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "k6_http_req_duration_p99",
          "legendFormat": "P99",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1000},
              {"color": "red", "value": 2000}
            ]
          },
          "unit": "ms"
        }
      }
    },
    {
      "id": 4,
      "title": "‚ùå Taxa de Erro (%)",
      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "sum(rate(k6_http_req_failed_rate[1m])) * 100",
          "legendFormat": "Error %",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          },
          "unit": "percent",
          "max": 100
        }
      }
    },
    {
      "id": 5,
      "title": "üñ•Ô∏è CPU Usage - Containers",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "rate(container_cpu_usage_seconds_total{name=~\"nginx|middleware\"}[1m]) * 100",
          "legendFormat": "{{name}} CPU %",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "percent",
          "max": 100
        }
      }
    },
    {
      "id": 6,
      "title": "üíæ Mem√≥ria Usage - Containers",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "container_memory_usage_bytes{name=~\"nginx|middleware\"} / 1024 / 1024",
          "legendFormat": "{{name}} Memory (MB)",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10
          },
          "unit": "decbytes"
        }
      }
    },
    {
      "id": 7,
      "title": "üìà Requisi√ß√µes HTTP ao Longo do Tempo",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "sum(rate(k6_http_reqs_total[1m]))",
          "legendFormat": "Total Requests/sec",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "sum(rate(nginx_http_requests_total[1m]))",
          "legendFormat": "Nginx Requests/sec",
          "refId": "B"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 0
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 8,
      "title": "üïê Tempo de Resposta por Percentil",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "k6_http_req_duration_p50",
          "legendFormat": "P50 (Mediana)",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "k6_http_req_duration_p90",
          "legendFormat": "P90",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "k6_http_req_duration_p99",
          "legendFormat": "P99",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 0
          },
          "unit": "ms"
        }
      }
    },
    {
      "id": 9,
      "title": "üåê Status das Conex√µes Nginx",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "nginx_connections_active",
          "legendFormat": "Conex√µes Ativas",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "nginx_connections_waiting",
          "legendFormat": "Conex√µes Aguardando",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "rate(nginx_connections_accepted[1m])",
          "legendFormat": "Conex√µes Aceitas/s",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 5
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 10,
      "title": "‚òï JVM - Middleware",
      "type": "timeseries",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "jvm_memory_used_bytes{area=\"heap\"} / 1024 / 1024",
          "legendFormat": "Heap Memory (MB)",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "jvm_threads_live_threads",
          "legendFormat": "Threads Ativas",
          "refId": "B"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 5
          }
        }
      }
    },
    {
      "id": 11,
      "title": "üìä Sistema - Vis√£o Geral",
      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "machine_cpu_cores",
          "legendFormat": "CPU Cores",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "machine_memory_bytes / 1024 / 1024 / 1024",
          "legendFormat": "RAM (GB)",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "up{job=\"nginx\"}",
          "legendFormat": "Nginx Status",
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "up{job=\"middleware\"}",
          "legendFormat": "Middleware Status",
          "refId": "D"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 1}
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "horizontal",
        "displayMode": "list"
      }
    },
    {
      "id": 12,
      "title": "üìã Resumo dos Testes K6",
      "type": "table",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "k6_http_reqs_total",
          "legendFormat": "{{method}} {{name}}",
          "refId": "A",
          "format": "table"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 26
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        }
      },
      "options": {
        "showHeader": true
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "__name__": true,
              "Time": true,
              "job": true,
              "instance": true
            },
            "indexByName": {},
            "renameByName": {
              "method": "M√©todo HTTP",
              "name": "Endpoint",
              "status": "Status Code",
              "Value": "Total Requests"
            }
          }
        }
      ]
    }
  ],
  "annotations": {
    "list": [
      {
        "name": "Test Start/End",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROMETHEUS}"
        },
        "enable": true,
        "expr": "changes(k6_vus[1m]) > 0",
        "iconColor": "blue",
        "titleFormat": "üë• VU Count Changed"
      }
    ]
  },
  "templating": {
    "list": []
  }
}